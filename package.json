    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4",
    "@types/bull": "^3.14.0",
    "@types/config": "0.0.36",
    "@types/express": "^4.17.2",
    "@types/jasmine": "^3.5.10",
    "jasmine": "^3.5.0",
    "nodemon": "^2.0.2"
  }
}
