{
  "name": "nest-skeleton",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/IVAgafonov/nest-skeleton.git"
  },
  "author": "IAgafonov",
  "license": "UNLICENSED",
  "type": "commonjs",
  "scripts": {
    "start-dev": "nodemon --config nodemon.json",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "start": "node ./build/start.js",
    "build:ts": "rm -rf ./build/ && tsc",
    "build": "npm run build:ts",
    "migration-generate": "ts-node node_modules/.bin/typeorm migration:create -f ./src/ormconfig.ts",
    "migration-apply": "ts-node node_modules/.bin/typeorm migration:run -f ./src/ormconfig.ts"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.5.11",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "config": "^3.2.6",
    "connect-timeout": "^1.9.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "log4js": "^6.3.0",
    "prom-client": "^12.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "ts-enums": "0.0.6",
    "yargs": "^15.3.1",
    "@nestjs/bull": "^0.1.2",
    "@nestjs/microservices": "^7.3.2",
    "@nestjs/typeorm": "^7.1.0",
    "amqp-connection-manager": "^3.2.0",
    "amqplib": "^0.5.6",
    "bull": "^3.15.0",
    "mysql": "^2.18.1",
    "numeral": "^2.0.6",
    "ts-option": "^2.1.0",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/jest": "25.2.3",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "create-ts-index": "^1.13.3",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
